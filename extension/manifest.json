{
"name": "Test Application",
"description": "Example of requests to Heroku using AXIOS",
"version": "1.0",
"manifest_version": 3,
"permissions": ["scripting"],
"background":{
  "service-worker": ["./node_modules/jquery/dist/jquery.min.js"]
},
"content_security_policy": {
  "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://test-python-heroku-app.herokuapp/data/;"
},
"content_scripts": 
    [
        {
            "matches":["https://test-python-heroku-app.herokuapp/data/"],
            "js":["./node_modules/jquery/dist/jquery.min.js", "script.js"]
        }
],
"action": {
        "default_popup": "source.html",
        "default_icon": {
      		"16": "/images/get_started16.png",
      		"32": "/images/get_started32.png",
      		"48": "/images/get_started48.png",
      		"128": "/images/get_started128.png"
	}
   },
"icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}